{
    "name": "josrom/laravel-analytics",
    "type": "library",
    "description": "Analytics tracking for Laravel",
    "keywords": [
        "analytics",
        "google analytics",
        "laravel",
        "statistics",
        "javascript",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Kummer",
            "email": "r.kummer@ipunkt.biz"
        },
        {
            "name": "Jose Vicente Orts",
            "email": "jvortsromero@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/config": "^9.40",
        "illuminate/session": "^9.40",
        "illuminate/support": "^9.40"
    },
    "require-dev": {
        "laravel/framework": "^9.40"
    },
    "autoload": {
        "psr-0": {
            "Josrom\\LaravelAnalytics\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "suggest": {
        "martijnc/php-csp": "For handling Content Security Policy matters"
    },
    "support": {
        "issues": "https://github.com/JoseVte/laravel-analytics/issues",
        "source": "https://github.com/JoseVte/laravel-analytics/archive/master.zip"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Josrom\\LaravelAnalytics\\AnalyticsServiceProvider"
            ],
            "aliases": {
                "Analytics": "Josrom\\LaravelAnalytics\\AnalyticsFacade"
            }
        }
    }
}
